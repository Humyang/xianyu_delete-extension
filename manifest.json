{
  "manifest_version": 2,
  "background": {
    "scripts": ["clock.js"]
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
    ,
    "activeTab",
    "notifications"
  ],
  "name": "Start Clock",
  "description": "定时提醒",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}